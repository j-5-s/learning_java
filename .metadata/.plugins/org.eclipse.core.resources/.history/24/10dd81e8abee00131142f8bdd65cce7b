package com.homedepot.headfirst;

public class ScratchPad {
	public void myMethod() {
		int myInt = 8;
		boolean myBoolean = true;
		String myString = "this is my string";
		char myChar =  'x';
		
		if ( myInt == 8 ) {
			myInt = 10;
		}
		
		System.out.print(myInt);
		System.out.println(myString);
		System.out.println(7);
	}
}
